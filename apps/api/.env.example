# Server
NODE_ENV=development
PORT=3001
CORS_ORIGIN=http://localhost:3000

# Avalanche
AVALANCHE_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc
PRIVATE_KEY=<filled>

# Contracts (already deployed)
QUERY_REGISTRY_ADDRESS=<filled>
AGENT_REGISTRY_ADDRESS=<filled>

# AI Services (choose one or both - DeepSeek is preferred if both set)
AI_PROVIDER=deepseek  # Options: openai, deepseek (auto-detects if not set)

# DeepSeek (recommended - cheaper)
DEEPSEEK_API_KEY=<filled>
DEEPSEEK_MODEL=deepseek-chat

# OpenAI (alternative)
OPENAI_API_KEY=<filled>
OPENAI_MODEL=gpt-4o-mini

# Data Sources
COINGECKO_API_URL=https://api.coingecko.com/api/v3

# x402 (Thirdweb)
THIRDWEB_SECRET_KEY=<filled>
THIRDWEB_CLIENT_ID=<filled>

# Payment
# PAYMENT_MODE: 'signature' (default, no real transfer) or 'real' (actual AVAX transfer)
PAYMENT_MODE=real
USDC_CONTRACT_ADDRESS=<filled>
PAYMENT_RECEIVER_ADDRESS=<filled>

